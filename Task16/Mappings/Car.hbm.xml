<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Task16"
                   namespace="Task16.Domain">
    <class name="Car" table="testdb.cars">
        <id name="Id" column="id" type="Int64">
            <generator class="sequence">
                <param name="sequence">testdb.cars_id_seq</param>
            </generator>
        </id>
        
        <property name="ModelName" column="model_name" type="String"></property>
        <property name="Year" column="year" type="Int32"></property>
        <property name="ModelClass"/>
        <component name="Owner">
            <property name="FirstName" column="owner_firstname"></property>
            <property name="LastName" column="owner_lastname"></property>
            <property name="Age" column="owner_age"></property>
        </component>

    </class>
</hibernate-mapping>